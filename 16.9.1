class Triangle:
    def __init__(self, a, b, footing, h, ):
        self.a = a
        self.b = b
        self.h = h
        self.footing = footing

    def __str__(self):
        return f"Triangle({self.a}, {self.b}, {self.footing}, {self.h})"

triangle_1 = Triangle(5, 15, 10, 25)

print(str(triangle_1))
